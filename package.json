{
    "name": "defycommerce",
    "version": "1.0.0",
    "private": true,
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "dependencies": {
        "@defycommerce/medusa": "workspace:*",
        "@defycommerce/frontend": "workspace:*"
    },
    "scripts": {
        "dev": "bun run --filter '*' dev",
        "build": "bun run --filter '*' build",
        "start": "bun run --filter '*' start"
    }
}